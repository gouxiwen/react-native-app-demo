{
  "name": "reactNativeApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --mode=DevDebug",
    "android-staging": "SET ENVFILE=.env.staging && react-native run-android --mode=StagingDebug",
    "android-staging:mac": "ENVFILE=.env.staging && react-native run-android --mode=StagingDebug",
    "android-prod": "SET ENVFILE=.env.prod && react-native  run-android --mode=ProdDebug",
    "bundle-android-dev": "cd android && SET ENVFILE=.env.dev && ./gradlew assembleDevRelease",
    "bundle-android-dev:mac": "cd android && ENVFILE=.env.dev && ./gradlew assembleDevRelease",
    "bundle-android-staging": "cd android && SET ENVFILE=.env.staging && ./gradlew assembleStagingRelease",
    "bundle-android-prod": "cd android && SET ENVFILE=.env.prod && ./gradlew assembleProdRelease",
    "bundle-android-aab-dev": "cd android && export ENVFILE=.env.dev ./gradlew bundleDevRelease",
    "bundle-android-aab-staging": "cd android && export ENVFILE=.env.staging ./gradlew bundleStagingRelease",
    "bundle-android-aab-prod": "cd android && export ENVFILE=.env.prod ./gradlew bundleProdRelease",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean:android": "cd android && ./gradlew clean"
  },
  "dependencies": {
    "@react-native-camera-roll/camera-roll": "^7.10.2",
    "@react-native-media-console/reanimated": "^2.2.4",
    "@react-native-vector-icons/ant-design": "^12.4.0",
    "@react-native/new-app-screen": "0.82.1",
    "@react-navigation/bottom-tabs": "^7.8.4",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "axios": "^1.13.2",
    "qs": "^6.14.0",
    "react": "19.1.1",
    "react-native": "^0.82.1",
    "react-native-config": "^1.6.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-fs": "^2.20.0",
    "react-native-media-console": "^2.2.4",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.18.0",
    "react-native-simple-toast": "^3.3.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-video": "^6.18.0",
    "react-native-worklets": "^0.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "reactotron-react-native": "^5.1.18",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
